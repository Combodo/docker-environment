{
  "version": 1,
  "root": {
    "level": "INFO",
    "handlers": ["console"]
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "simple"
    }
  },
  "formatters": {
    "simple": {
      "format": "%(asctime)s - %(levelname)s - %(message)s"
    }
  }
}
